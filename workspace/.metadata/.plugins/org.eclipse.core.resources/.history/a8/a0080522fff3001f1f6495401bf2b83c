<mapper namespace="com.myweb.board.model.NoticeBoard">
	<!--
	id는 인터페이스의 함수명
	세미콜론은 적지 않습니다.
	
	select는 resultype에 반환받을 타입을 반드시 적어야함
	타입의 종류 - 스트링, 인트, 더블, 어레이, dto 
	-->
		
	<select id="getTime" resultype = "string">
		SELECT SYSDATE FROM DUAL
	</select>
</mapper>
